{
    "name": "conceptlz/trident-plans",
    "description": "SaaS style recurring plans for Laravel.",
    "keywords": ["trident", "plans", "laravel", "subscriptions", "memberships"],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^7.4|^8.0",
        "nesbot/carbon": "^2.0",
        "illuminate/contracts": "^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "orchestra/testbench": "~3.8.0",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~7.5"
    },
    "autoload": {
        "psr-4": {
            "Gerardojbaez\\Laraplans\\": "src/Laraplans/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ],
        "psr-4": {
            "Gerardojbaez\\Laraplans\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev"
}
